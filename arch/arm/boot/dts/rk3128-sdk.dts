/dts-v1/;

#include "rk3128.dtsi"
#include "rk3128-cif-sensor.dtsi"
#include "rk312x-sdk.dtsi"
#include "lcd-86v-rgb1024x600.dtsi"

/ {
	 compatible = "rockchip,rk3128";
        backlight: backlight {
                compatible = "pwm-backlight";
                pwms = <&pwm0 0 25000>;
                brightness-levels = <128 129 130 131 132 133 134 135 136 137 138 139 140 141 142 143 144 145 146 147 148 149 150 151 152 153 154 155 156 157 158 159 160 161 162 163 164 165 166 167 168 169 170 171 172 173 174 175 176 177 178 179 180 181 182 183 184 185 186 187 188 189 190 191 192 193 194 195 196 197 198 199 200 201 202 203 204 205 206 207 208 209 210 211 212 213 214 215 216 217 218 219 220 221 222 223 224 225 226 227 228 229 230 231 232 233 234 235 236 237 238 239 240 241 242 243 244 245 246 247 248 249 250 251 252 253 254 255>;
                default-brightness-level = <254>;
              //  enable-gpios = <&gpio0 GPIO_D2 GPIO_ACTIVE_HIGH>;
        };

	usb_control {
		compatible = "rockchip,rk3126-usb-control";

		//host_drv_gpio = <&gpio3 GPIO_C4 GPIO_ACTIVE_LOW>;
		//otg_drv_gpio = <&gpio3 GPIO_C1 GPIO_ACTIVE_LOW>;

		rockchip,remote_wakeup;
		rockchip,usb_irq_wakeup;
	};
	wireless-wlan {
		compatible = "wlan-platdata";

		/* wifi_chip_type - wifi chip define
                 * ap6210, ap6330, ap6335
                 * rtl8188eu, rtl8723bs, rtl8723bu rtl8189fs
                 * esp8089
		*/
		wifi_chip_type = "rtl8189fs";
		sdio_vref = <3300>; //1800mv or 3300mv

		//	power_ctrl_by_pmu;
		// 	pmu_regulator = "act_ldo3";
		// 	pmu_enable_level = <1>; //1->HIGH, 0->LOW
		/* WIFI_HOST_WAKE = GPIO3_C7 */
		WIFI,host_wake_irq = <&gpio3 GPIO_C7 GPIO_ACTIVE_HIGH>;
		/* WIFI_REG_ON 	= GPIO3_D3 */
		WIFI,poweren_gpio  = <&gpio3 GPIO_D3 GPIO_ACTIVE_HIGH>;

		status = "okay";
	};
	wireless-bluetooth {
		compatible = "bluetooth-platdata";

		//wifi-bt-power-toggle;

		uart_rts_gpios = <&gpio0 GPIO_C1 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default","rts_gpio";
		pinctrl-0 = <&uart0_rts>;
		pinctrl-1 = <&uart0_rts_gpio>;


		/* BT_HOST_WAKE = GPIO3_C6 */
		/* BT_RST 		= GPIO3_C5 */
		/* BT_WAKE 		= GPIO3_D2 */
		//BT,power_gpio = <&gpio4 GPIO_D3 GPIO_ACTIVE_HIGH>;
		BT,reset_gpio = <&gpio3 GPIO_C5 GPIO_ACTIVE_HIGH>;
		//BT,wake_gpio = <&gpio3 GPIO_D2 GPIO_ACTIVE_HIGH>;
		BT,wake_host_irq = <&gpio3 GPIO_C6 GPIO_ACTIVE_LOW>;

		status = "disabled";
	};
	
		

	rockchip_suspend: rockchip_suspend {
                           rockchip,ctrbits = <
                           (0
                           |RKPM_CTR_PWR_DMNS
                           |RKPM_CTR_GTCLKS
                           |RKPM_CTR_PLLS
                          |RKPM_CTR_ARMOFF_LPMD
                         |RKPM_CTR_IDLESRAM_MD
                           |RKPM_CTR_DDR
                          |RKPM_CTR_VOLTS
                           |RKPM_CTR_BUS_IDLE
                   //        |RKPM_CTR_VOL_PWM1
                           )
                   >;
          rockchip,pmic-suspend_gpios = <0>;//GPIO3_C1
     };
     
	pwmleds {
		compatible = "pwm-leds";
		green_gpio = <&gpio0 GPIO_D4 GPIO_ACTIVE_HIGH>;
		blue_gpio = <&gpio0 GPIO_D3 GPIO_ACTIVE_HIGH>;
		red_gpio = <&gpio2 GPIO_D1 GPIO_ACTIVE_HIGH>;
		
	};
};



&pwm1 {
  status = "okay";
};

&pwm2 {
  status = "okay";
};

&uart0 {
        status = "okay";
        dma-names = "!tx", "!rx";
        pinctrl-0 = <&uart0_xfer>;
};

&uart1{
	status = "okay";
	dma-names = "!tx", "!rx";
	pinctrl-0 = <&uart1_xfer>;
};




&fb {
	rockchip,disp-mode = <NO_DUAL>;
	rockchip,uboot-logo-on = <1>;
};

&rk_screen {
	display-timings = <&disp_timings>;
};

&lvds {
	status = "okay";

	pinctrl-names = "lcdc";
	pinctrl-0 = <&lcdc0_lcdc_d>;
};

&lcdc {
	status = "okay";

	backlight = <&backlight>;
	pinctrl-names = "default", "gpio";
	pinctrl-0 = <&lcdc0_lcdc>;
	pinctrl-1 = <&lcdc0_gpio>;
  cs-gpio = <&gpio1 GPIO_B3 GPIO_ACTIVE_HIGH>;
  sdi-gpio = <&gpio1 GPIO_B1 GPIO_ACTIVE_HIGH>;
  scl-gpio = <&gpio1 GPIO_B0 GPIO_ACTIVE_HIGH>;
	rst-gpio = <&gpio0 GPIO_D0 GPIO_ACTIVE_HIGH>;
	
	rockchip,fb-win-map = <FB_DEFAULT_ORDER>;
	power_ctr: power_ctr {
		rockchip,debug = <1>;
		
		lcd_cs: lcd_cs {
			rockchip,power_type = <REGULATOR>;
			rockchip,regulator_name = "rk816_ldo1";
			rockchip,regulator_voltage = <2800000>;
			rockchip,delay = <10>;
		};
	
		lcd_en: lcd_en {
			rockchip,power_type = <GPIO>;
			gpios = <&gpio2 GPIO_B3 GPIO_ACTIVE_HIGH>;
			rockchip,delay = <10>;
		};	
	};
};

&hdmi {
        status = "disabled";
};


&dwc_control_usb {
	usb_uart {
		status = "disabled";
	};
};

&pinctrl {
		init-gpios = <&gpio0 GPIO_B5 1/*13 auxin_en*/
		&gpio3 GPIO_C5 1/*117 music led*/
		&gpio3 GPIO_D2 0/*122 laohu*/
		/*&gpio1 GPIO_A7 1spk*/
		&gpio1 GPIO_B1 0
		&gpio1 GPIO_B0 0
		&gpio1 GPIO_B3 0
		&gpio0 GPIO_D0 1
		&gpio2 GPIO_D1 1/*89 LED*/
		&gpio0 GPIO_D3 1/*27 LED*/
		&gpio0 GPIO_D4 1/*28 LED*/
		&gpio0 GPIO_D6 0/*30 CTL_1 CODEC*/
		&gpio1 GPIO_A3 0/* 35 CTL_2 CODEC*/
		&gpio3 GPIO_C4 1 /* 116 hp con*/
		&gpio0 GPIO_B6 GPIO_ACTIVE_HIGH>;
};


&codec {
	spk_ctl_io = <&gpio1 GPIO_A7 GPIO_ACTIVE_HIGH>;
	hp_ctl_io  = <&gpio3 GPIO_C4 GPIO_ACTIVE_HIGH>;
        spk-mute-delay = <50>;
        hp-mute-delay = <10>;
        rk312x_for_mid = <1>;
        is_rk3128 = <1>;
        spk_volume = <26>;
        hp_volume = <26>;
        capture_volume = <23>;
        gpio_debug = <1>;
        codec_hp_det = <1>;
};

&rk3128_cif_sensor{
	status = "disabled";
};


